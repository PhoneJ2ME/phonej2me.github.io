+     { if (SDL_WaitEvent(&event))
+          CheckEvent(&event, pNewSignal, pNewMidpEvent);
+       return;
+     }
+  do { if (SDL_PollEvent(&event))
+          { CheckEvent(&event, pNewSignal, pNewMidpEvent);
+            return;
+          }
+       stopTime = JVM_JavaMilliSeconds();
+     } while((stopTime - currentTime) < timeout);
+}
