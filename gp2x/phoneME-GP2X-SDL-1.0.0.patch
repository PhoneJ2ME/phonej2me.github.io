+  GP2X_Default_keys[8] = KEYMAP_KEY_SOFT2;
+  GP2X_Default_keys[9] = KEYMAP_KEY_SOFT1;
+  GP2X_Default_keys[10] = KEYMAP_KEY_POUND;
