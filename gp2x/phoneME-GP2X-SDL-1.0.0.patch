+	while (cc--)
+	  {
+	    s = *sp++;
+	    MIXATION(left);
+	    lp++;
+	  }
+	cc = song->control_ratio;
+	if (update_signal(song, v))
+	  return;	/* Envelope ran out */
+	left = vp->left_mix;
+      }
+    else
+      {
+	vp->control_counter = cc - count;
+	while (count--)
+	  {
+	    s = *sp++;
+	    MIXATION(left);
+	    lp++;
+	  }
+	return;
+      }
+}
