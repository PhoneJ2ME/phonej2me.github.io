+class EventQueueEntry {
+    String name;
+    Object data;
+    EventQueueEntry link;
+    
+    public EventQueueEntry(String n, Object d) {
+        name = n;
+        data = d;
+        link = this;
+    }
+    public EventQueueEntry(String n, Object d, EventQueueEntry l) {
+        name = n;
+        data = d;
+        link = l;
+    }
