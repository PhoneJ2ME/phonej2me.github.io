+     }
+  if ((event->type == SDL_KEYDOWN) || (event->type == SDL_KEYUP))
+     { KeyboardCheck(event, pNewSignal, pNewMidpEvent);
+       return;
+     }
+  if ((event->type == SDL_JOYBUTTONDOWN) || (event->type == SDL_JOYBUTTONUP))
+     { JoystickCheck(event, pNewSignal, pNewMidpEvent);
+       return;
+     }
+}
