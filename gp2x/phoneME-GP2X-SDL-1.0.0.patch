+  SUBSYSTEM_AMS_JAVA_FILES += \
+    $(SUBSYSTEM_DIR)/ams/example/jams/classes/com/sun/midp/scriptutil/HttpJadInstaller.java \
+    $(SUBSYSTEM_DIR)/ams/example/jams/classes/com/sun/midp/scriptutil/SuiteLister.java \
