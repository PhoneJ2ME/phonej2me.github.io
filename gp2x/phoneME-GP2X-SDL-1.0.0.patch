+static void mix_mono_signal(MidiSong *song, sample_t *sp, Sint32 *lp, int v,
+			    int count)
+{
+  Voice *vp = song->voice + v;
+  final_volume_t 
+    left=vp->left_mix;
+  int cc;
+  sample_t s;
+  
