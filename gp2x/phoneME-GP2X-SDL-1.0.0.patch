+static void free_instrument(Instrument *ip)
+{
+  Sample *sp;
+  int i;
+  if (!ip) return;
+  for (i=0; i<ip->samples; i++)
+    {
+      sp=&(ip->sample[i]);
+      free(sp->data);
+    }
+  free(ip->sample);
+  free(ip);
+}
